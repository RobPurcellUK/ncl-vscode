{
    "name": "ncl",
    "displayName": "NCL for vscode",
    "description": "NCAR Command Language support for vscode",
    "version": "0.0.1",
    "publisher": "robpurcell",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ncl",
            "aliases": ["NCL", "ncl"],
            "extensions": [".ncl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ncl",
            "scopeName": "source.ncl",
            "path": "./syntaxes/NCL.tmLanguage"
        }]
    }
}